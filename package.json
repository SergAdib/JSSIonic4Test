{
  "name": "ionic4",
  "version": "0.0.1",
  "author": "Ionic Framework",
  "homepage": "http://ionicframework.com/",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:disconnected": "npm-run-all --serial bootstrap:disconnected --parallel start:angular start:proxy start:watch-components",
    "start:connected": "scjss-verify-setup && npm-run-all --serial bootstrap:connected start:angular start:watch-components",
    "build1": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "jss": "jss",
    "build": "npm-run-all --serial bootstrap:connected --parallel build:client build:server --serial build:server-wrapper",
    "build:cordova": "ng build --prod --base-href . --output-path platforms/android/platform_www/",
    "scaffold": "ng generate @sitecore-jss/sitecore-jss-angular-schematics:jss-component",
    "start:angular": "ng serve -o --base-href=/",
    "start:proxy": "ts-node scripts/disconnected-mode-proxy.ts",
    "start:watch-components": "ts-node scripts/generate-component-factory.ts --watch",
    "build:client": "cross-env-shell ng build --configuration=production --deploy-url $npm_package_config_sitecoreDistPath/browser/ --output-path=$npm_package_config_buildArtifactsPath/browser/",
    "build:server": "cross-env-shell ng run ionic4:server --output-path=$npm_package_config_buildArtifactsPath/server --output-hashing=none",
    "build:server-wrapper": "cross-env-shell webpack --config webpack.server.config.js --progress --colors --output-path $npm_package_config_buildArtifactsPath",
    "bootstrap:connected": "ts-node scripts/bootstrap.ts",
    "bootstrap:disconnected": "ts-node scripts/bootstrap.ts --disconnected",
    "graphql:update": "ts-node ./scripts/update-graphql-fragment-data.ts",
    "iostart": "ionic-app-scripts serve",
    "ioclean": "ionic-app-scripts clean",
    "iobuild": "ionic-app-scripts build",
    "iolint": "ionic-app-scripts lint"
  },
  "private": true,
  "config": {
    "appName": "ionic4",
    "rootPlaceholders": [
      "jss-main"
    ],
    "sitecoreDistPath": "/dist/ionic4",
    "sitecoreConfigPath": "/App_Config/Include/zzz",
    "buildArtifactsPath": "./dist",
    "graphQLEndpointPath": "/api/ionic4",
    "language": "en"
  },
  "dependencies": {
    "@angular/animations": "~6.1.1",
    "@angular/common": "~6.1.1",
    "@angular/compiler": "~6.1.1",
    "@angular/core": "~6.1.1",
    "@angular/forms": "~6.1.1",
    "@angular/http": "~6.1.1",
    "@angular/platform-browser": "~6.1.1",
    "@angular/platform-browser-dynamic": "~6.1.1",
    "@angular/platform-server": "~6.1.1",
    "@angular/router": "~6.1.1",
    "@ionic-native/core": "5.0.0-beta.15",
    "@ionic-native/splash-screen": "5.0.0-beta.15",
    "@ionic-native/status-bar": "5.0.0-beta.15",
    "@ionic/angular": "4.0.0-beta.7",
    "@ng-toolkit/universal": "^1.1.41-beta.110",
    "@nguniversal/module-map-ngfactory-loader": "^6.0.0",
    "@ngx-meta/core": "^6.0.0-rc.1",
    "@ngx-translate/core": "^10.0.2",
    "@ngx-translate/http-loader": "^3.0.1",
    "@sitecore-jss/sitecore-jss": "^9.0.5",
    "@sitecore-jss/sitecore-jss-angular": "^9.0.5",
    "apollo-angular": "^1.1.1",
    "apollo-angular-link-http-batch": "^1.1.1",
    "apollo-angular-link-persisted": "^1.0.0-beta.2",
    "apollo-cache-inmemory": "^1.2.5",
    "apollo-client": "^2.3.5",
    "apollo-link": "^1.2.2",
    "bootstrap": "^4.1.1",
    "cordova-android": "7.0.0",
    "cordova-browser": "5.0.4",
    "cordova-plugin-device": "^2.0.2",
    "cordova-plugin-ionic-keyboard": "^2.1.2",
    "cordova-plugin-ionic-webview": "^2.1.4",
    "cordova-plugin-splashscreen": "^5.0.2",
    "cordova-plugin-statusbar": "^2.4.2",
    "cordova-plugin-whitelist": "^1.3.3",
    "core-js": "^2.5.3",
    "graphql": "0.13.0",
    "graphql-tag": "^2.9.2",
    "lodash": "^4.17.10",
    "rxjs": "6.3.2",
    "rxjs-compat": "^6.0.0-rc.0",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/architect": "~0.7.2",
    "@angular-devkit/build-angular": "~0.7.2",
    "@angular-devkit/core": "~0.7.2",
    "@angular-devkit/schematics": "~0.7.2",
    "@angular/cli": "~6.1.1",
    "@angular/compiler": "~6.1.1",
    "@angular/compiler-cli": "~6.1.1",
    "@angular/language-service": "~6.1.1",
    "@ionic/ng-toolkit": "^1.0.0",
    "@ionic/schematics-angular": "^1.0.0",
    "@sitecore-jss/sitecore-jss-angular-schematics": "^9.0.5",
    "@sitecore-jss/sitecore-jss-cli": "^9.0.6",
    "@sitecore-jss/sitecore-jss-dev-tools": "^9.0.6",
    "@sitecore-jss/sitecore-jss-manifest": "^9.0.5",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^10.10.1",
    "body-parser": "^1.18.2",
    "chalk": "^2.1.0",
    "codelyzer": "~4.4.2",
    "cookie-parser": "^1.4.3",
    "cross-env": "^5.0.5",
    "enhanced-resolve": "3.3.0",
    "express": "^4.16.2",
    "fs-extra": "^5.0.0",
    "isomorphic-fetch": "^2.2.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "~2.0.0",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "npm-run-all": "^4.1.3",
    "protractor": "~5.4.0",
    "ts-loader": "^4.3.1",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~2.9.2",
    "webpack-cli": "^3.0.2"
  },
  "description": "An Ionic project",
  "cordova": {
    "plugins": {
      "cordova-plugin-whitelist": {},
      "cordova-plugin-statusbar": {},
      "cordova-plugin-device": {},
      "cordova-plugin-splashscreen": {},
      "cordova-plugin-ionic-webview": {
        "ANDROID_SUPPORT_ANNOTATIONS_VERSION": "27.+"
      },
      "cordova-plugin-ionic-keyboard": {}
    },
    "platforms": [
      "android",
      "browser"
    ]
  }
}